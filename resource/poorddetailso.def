product ZABDB 

table poorddetailso
  caption "PO Detail"
  columns ztime,zutime,zauserid,zuuserid,zid,~
          xpornum,xrow, xitem,xnote, ~
          xqtyord,xrate,xunitpur,xlineamt,xvatrate,xvatamt,~
          xqtybonus,xqtygrn,xdisc,xdiscf,xfatype,xcat,xcfpur,xdocrow,xspecification,xbrand,xrategrn,xserial,xbase,xstatus,xmodel,~
		  xprodyear,xbuyeramt,xcolor,xcapacity,xorigin,xprimebuyer,xttamount,xttbuyer,xttbase,xqtyprn

  primary key zid,xpornum,xrow

  foreign key
        zid references zbusiness.zid
        zid,xpornum references poordheader.zid,xpornum
        zid,xitem references caitem.zid,xitem
  end foreign key

end table
