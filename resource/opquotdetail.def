product ZAB ERP

table opquotdetail
  caption "Quotation Detail"
  columns ztime,zutime,zauserid,zuuserid,zid,xqotnum,xrow,xitem,xqtyord,xnote, ~
          xunitsel,xrate,xlineamt,xwperiod,xwperiodtype

  primary key zid,xqotnum,xrow
  foreign key
        zid references zbusiness.zid
        zid,xqotnum references opquotheader.zid,xqotnum
        zid,xitem references caitem.zid,xitem
  end foreign key
end table
