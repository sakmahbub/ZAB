product ZAB ERP

table ppoview
  caption "PPO View"
  columns zid,xtype,xlong,xcode

  primary key zid,xcode

  foreign key
        zid references zbusiness.zid
  end foreign key

	sql CREATE VIEW ppoview(zid,xsp,xname,xwh,xterritory) ~
	AS ~
	SELECT zid,xsp,xname,xwh,xterritory ~
	FROM cappo Where Left(xsp,4)='SPR-'

end table

