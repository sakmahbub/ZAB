     list cabanknew
        table cabankviewnew
        order xbank
		linksql select xname from cabankviewnew 
        select "(xbank like '%"+zvalue+"%' or xname like '%"+zvalue+"%') and xacc<>'' and xacc<>'0'"
        rows 24
        objects xbank attrib(link "abc" embed onclick="return updateParent(this:'')"), ~
                xname,xbacc,xbranch,xmadd,xacc,xswiftcode
     end list


