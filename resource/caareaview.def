Product ZAB ERP

table caareaview
  caption "Area View"
  columns zid,xtype,xcode
  primary key zid,xtype,xcode

  foreign key
        zid references zbusiness.zid
  end foreign key

  sql create view caareaview (zid,xtype,xcode)~
                  as select zid,xtype,xcode ~
                  from xcodes where xtype = 'Area'


end table
