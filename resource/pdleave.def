product ZAB ERP

table pdleave
  caption "Personal Assigned Leave"
  columns ztime,zutime,zauserid,zuuserid,zid,xyear,xstaff,xtypeleave,xavailable,xahour,xused,xuhour,xbalance,xbhour,xstatus

  primary key zid,xyear,xstaff,xtypeleave

  foreign key
      zid references zbusiness.zid
      zid,xstaff references pdmst.zid,xstaff
  end foreign key

  field xavailable
    caption Available Leave
  end field
  field xused
    caption Used Leave
  end field
  field xbalance
    caption Balance
  end field
end table
