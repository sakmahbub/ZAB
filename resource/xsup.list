     list xsup
        table cacus
        order xcus
		linksql select xorg from cacus
        select "((xcus like '%"+zvalue+"%' or xorg like '%"+zvalue+"%') and xtype not in('Customer','Buyer'))"
        rows 10
		searchselect "xtype not in('Customer','Buyer')"        
        objects xsup attrib(link "abc" embed onclick="return updateParent(this:'')"), ~
                xorg,xtype,xmadd



        //totals "","Totals","","","","",sum,sum

        //headers none
     end list


