list xcus
caption "Customer containing: "+zvalue
  table cacus
  order xcus
   linksql select xorg from cacus
   select "((xcus like '%"+zvalue+"%' or xorg like '%"+zvalue+"%') and xtype in('Customer','Both'))"
        rows 10
	 searchselect "xtype in('Customer','Both')"
		objects xcus attrib(link "abc" embed onclick="return updateParent(this:'$')"), ~
          xorg,xmadd,xtype,xgcus

  header "Customer ID","Customer Name","Mailing Address","Party Type","Group"
        
end list


   
  
 