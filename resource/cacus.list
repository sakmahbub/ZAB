list cacus
  table cacus
  order xcus
  linksql select xorg from cacus
	select "((xcus like '%"+zvalue+"%' or xorg like '%"+zvalue+"%') and xtype in('Customer','Both'))"
	rows 20
	searchselect "xtype in('Customer','Both')"
	objects xcus attrib(link "abc" embed onclick="return updateParent(this:'xcus')"), ~
	xorg,xmadd,xtype,xgcus

  header "Customer ID","Customer Name","Address","Party Type","Group"
        
end list


