product ZAB ERP

table motrn
  caption "Production Transaction"
  columns ztime,zutime,zauserid,zuuserid,zid,xmotrnnum,xwh,xitem,xmoprcs,xdate,~
          xqty,xval,xvalpost,xdocnum,xdocrow,xnote,~
          xsign,xunit,xrate,xref,xbag,xlotno,xrow,xpiref,xbatch,xcqtyuse,xissuetype,xcostrow,~
		  xlot,xstatus,xcus,xsup

  primary key zid,xmotrnnum

