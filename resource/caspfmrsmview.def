
product ZAB ERP

table caspfmrsmview

  columns zid,xsp,xname,xwh
  primary key zid,xsp

  foreign key
        zid references zbusiness.zid
  end foreign key

  sql create view caspfmrsmview (zid,xsp,xname,xwh)~
                  as select zid,xfm,xname,xwh ~
                  from cafm where zactive='1' ~	

		  union all ~

		  select zid,xrsm,xname,xwh ~
                  from carsm where zactive='1' 	


