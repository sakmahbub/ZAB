list cabuyer
caption "Buyer List"
  table cacus
  order xcus
  select "((xcus like '%"+zvalue+"%' or xorg like '%"+zvalue+"%') and xgcus='Buyer')"
  rows 10
  searchselect "xgcus='Buyer'"
  objects xcus attrib(link "abc" embed onclick="return updateParent(this:'')"),~
          xorg,xmadd
        
end list


