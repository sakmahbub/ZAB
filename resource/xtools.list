     list xtools
        caption "Items containing: "+zvalue
        table caitem
        order xitem
		linksql select xdesc from caitem
        select "((xitem like '%"+zvalue+"%' or xdesc like '%"+zvalue+"%') and left(xitem,2)<>'CC' and xtypeitem='Tools')"
        searchselect "left(xitem,2)<>'CC' and xtypeitem='Tools'"
		rows 30
        objects xitem attrib(link "abc" embed onclick="return updateParent(this:'$')"), ~
                xdesc,xtheircode,xcatitem//,xitemdept,xitemsubdept//,xshelf
     end list


