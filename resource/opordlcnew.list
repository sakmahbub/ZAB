 list opordlcnew
        table lcinfo
        order xlcno
        select "(xlcno like '%"+zvalue+"%' and xtype='Export' and xordertype in ('LC','SC'))"
		searchselect "xtype='Export' and xordertype in( 'LC','SC')"
        rows 10
        objects xlcno attrib(link "abc" embed onclick="return updateParent(this:'')"),~
		xcus,xlcissuedate,xscref
	headers "LC/SC No.","Customer ID","LC/SC Issue Date","SC Ref No."
     end list