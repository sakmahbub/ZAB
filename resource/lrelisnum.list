     list lrelisnum
        caption "Loan Receive/Issue No List"</b>"
        table imtorheader
        order xtornum
        select "(xtornum like '%"+zvalue+"%' and left(xtornum,4) in ('LRE-','LIS-'))"
		searchselect "left(xtornum,4) in ('LRE-','LIS-')"
        rows 10
        objects ~
         xtornum attrib(link "abc" embed onclick="return updateParent(this:'')"),xdate,xloannum,xacc
		 
		 headers "Loan Receive/ Issue No", "Date","Loan Request No","Party Code"
     end list


