
product ZAB ERP

table opdosumview

  caption "Ac Bal View"

  columns zid,xacc,xsub,xyear,xprime,xacctype,xwh,xhwh

  primary key zid,xacc,xsub,xyear

  foreign key
        zid references zbusiness.zid
  end foreign key

  sql create view opdosumview (zid,xwh,xterritory,xsumnum,xdatedel,xpaymentterm,xrow)~
                as select zid,xwh,xterritory,xsumnum,xdatedel,xpaymentterm,count(*) ~
                from opdoheader ~
		where xstatusord='Open' and xsumnum<>'' ~
		group by zid,xwh,xterritory,xsumnum,xdatedel,xpaymentterm		  

			

end table
