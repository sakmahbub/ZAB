product ERP on the Net

table opordheadersoview
  caption "So Header"
   columns zid,xordernum,xorderdate,xpornum,xcus,xorg,xstatusord,xstatusdor,xstatusso,xdatedel,xccontract,xbookingno,xstyleno
  primary key zid,xordernum,xorderdate,xpornum,xcus,xorg,xstatusord,xstatusdor,xstatusso,xdatedel,xccontract,xbookingno,xstyleno
  foreign key
        zid references zbusiness.zid
  end foreign key
 sql create view opordheadersoview (zid,xordernum,xorderdate,xpornum,xcus,xorg,xstatusord,xstatusdor,xstatusso,xdatedel,xccontract,xbookingno,xstyleno) ~
                  as select zid,xordernum,xorderdate,xpornum,xcus,(select xorg from cacus where zid=opordheaderso.zid and  xcus=opordheaderso.xcus) as xorg ,xstatusord,xstatusdor,xstatusso,xdatedel,ISNULL(xccontract,''),ISNULL(xbookingno,''),ISNULL(xstyleno,'') from opordheaderso 

end table
	 

