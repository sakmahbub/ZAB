product ZAB ERP

table cacusview
  caption "OP DO CRN View"

  columns zid,xcus,xorg,xmadd,xgcus


  primary key zid,xcus

  foreign key
        zid references zbusiness.zid
  end foreign key

  sql create view cacusview(zid,xcus,xorg,xmadd,xgcus) ~

                  as select a.zid,a.xcus,a.xorg,a.xmadd,a.xgcus ~
		        from cacus a  where a.xtype='Customer'


end table
