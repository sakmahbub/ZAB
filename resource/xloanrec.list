     list xloanrec
        caption "Loan Return List"</b>"
        table imtorheader
        order xtornum desc
		linkobjects xacc
        select "(xtornum like '%"+zvalue+"%' and left(xtornum,4)='LIS-' and xstatusloan='Open' and xtype='Loan Issue')"
		searchselect "left(xtornum,4)='LIS-' and xstatusloan='Open' and xtype='Loan Issue'"
        rows 10
        objects ~
         xtornum attrib(link "abc" embed onclick="return updateParent(this:'')"),xdate,xordernum,xacc
		 
		headers "Loan Number","Date" ,"Ref","Party"
     end list


