product ZAB ERP

table caspmsoview
  caption "Credit Limit View"
  columns zid,xsp,xname,xwh
  primary key zid,xsp

  foreign key
        zid references zbusiness.zid
  end foreign key

  sql create view caspmsoview (zid,xsp,xname,xwh)~
                  as select zid,xsp,xname,xwh ~
                  from casp where left(xsp,4)='PPO-' or left(xsp,4)='SPR-'

end table
