 list xbcolor
	 caption "Buyer Color List"
        table cacolorview
        order xcolorcode,xbcolor
		linkobjects xcolorcode
		linksql select xcolorcode from cacolorview
        select "(xbcolor like '%"+zvalue+"%' or xcolorcode like '%"+zvalue+"%')"
        rows 20
        objects xbcolor attrib(link "abc" embed onclick="return updateParent(this:'')"), ~
			xcolorcode
               
	headers "Byer Color","GUL Color Code"
     end list