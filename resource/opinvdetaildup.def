product ZAB ERP

table opinvdetaildup
  caption "Commercial Invoice Detail"
  columns ztime,zutime,zid,zauserid,zuuserid,~
         xinvnumdup, xinvnum,xinvrow,xdornum,xitem,xnote,xqtyinv,xrate,xlineamt,~
		  xpiref,xpidate,xgrossweight,xnoofrolls,xorderrow,xprcstype,xstyle,~
		  xcomposition,xfabrictype,xfabriccolor,xwidth,xgsm,~
		  xordernum,xnetweight,xctnno,xprodtype,xcolorcode,xbcolor,xordernumso,xpartialchano

  primary key zid,xinvnumdup,xinvrow

  foreign key
        zid references zbusiness.zid
        zid,xinvnumdup references opinvheaderdup.zid,xinvnumdup
       zid,xitem references caitem.zid,xitem
  end foreign key

end table

