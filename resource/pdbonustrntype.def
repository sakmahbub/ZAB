
product ZAB ERP

table pdbonustrntype
  caption "Leave View"
  columns zid,xcode,xprops

  primary key zid,xcode

  foreign key
        zid references zbusiness.zid
  end foreign key

  sql create view pdbonustrntype (zid,xcode,xprops)~
                  as select zid,xcode,xprops from xcodes where xtype='PD Transaction Type' and ~
                  xlong='Bonus & Other Charges'
end table
