list xcount
        table xcodes
        order xcode
        select "(xtype = 'Count') and (xcode like '%"+zvalue+"%')and zactive=1" 		
        rows 20
		searchselect "xtype = 'Count' and zactive=1" 		
        objects xcode attrib(link "abc" embed onclick="return updateParent(this:'')"),~
		xlong
         header "Count","Note"       
end list



