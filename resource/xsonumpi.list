list xsonumpi
        caption "Items containing: "+zvalue
        table opordheaderso
        order xordernum
        select "(xordernum like '%"+zvalue+"%' or xorderdate like '%"+zvalue+"%') and xtype='Main' and xstatusord='Confirmed' and xinvnum=''"
        searchselect "xtype='Main' and xinvnum=''"
		rows 20
        objects xordernum attrib(link "abc" embed onclick="return updateParent(this:'$')"), ~
                xorderdate,xpornum,xcus,desc equals((select xorg from cacus where zid=opordheaderso.zid and xcus=opordheaderso.xcus))
				
		headers "SO No","Date","PO No","Customer ID","Customer Name"	
 end list 