


product ZAB ERP

table cussupstaffview
  caption "Ac Sub View"
  columns zid,xacc,xyear,xcur,xdiv,xsec,xproj,xprime,xbase,xacctype
  primary key zid,xacc,xyear,xcur,xdiv,xsec,xproj
  foreign key
        zid references zbusiness.zid
  end foreign key

  sql create view cussupstaffview (zid,xstaff,xname,xmadd)~
                  as select a.zid,a.xcus,a.xorg,a.xmadd from cacus a ~
				  union all ~
				  select a.zid,a.xstaff,a.xname,a.xmadd from pdmst a 
				  end table
