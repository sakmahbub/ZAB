 list buyercolor
        table cacolorview
        order xbcolor
			// select "((xcus like '%"+zvalue+"%' or xorg like '%"+zvalue+"%') and xtype in('Customer','Buyer','Both'))"
        select "(xcolorcode like '%"+zvalue+"%')"
		
        rows 20
        objects xbcolor attrib(link "abc" embed onclick="return updateParent(this:'xbcolor')"),~
               xcolorcode
		 header "Buyer Color","Color Code"
     end list