     list xroster
        table pdrosterheader
        order xroster desc
        select "xroster like '%"+zvalue+"%' and left(xroster,4)='RSTR'"
		searchselect "left(xroster,4)='RSTR'"
        rows 20
        objects xroster attrib(link "abc" embed onclick="return updateParent(this:'')"),~
                xgroup,xshift,xstaff,xfdate,xtdate

     end list


