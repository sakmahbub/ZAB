
product ZAB ERP

table pdmstnotcntview
  caption "Not contract View"
  columns zid,xstaff,xname,xdeptname,xposition,xdesignation,~
                xempstatus,xempcategory

  primary key zid,xstaff

  foreign key
        zid references zbusiness.zid
  end foreign key

  sql create view pdmstnotcntview (zid,xstaff,xname,xmadd,xpadd,xdeptname,xposition,xdesignation,~
                                xempstatus,xempcategory) ~
                  as select zid,xstaff,xname,xmadd,xpadd,xdeptname,xposition,xdesignation,~
                            xempstatus,xempcategory ~
                  from pdmst where xempcategory not in('Contract','Casual') and xempstatus='Normal'
end table
