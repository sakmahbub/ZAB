


product ZAB ERP

table imtorreqview
  caption "Store Requisition View"

  foreign key
        zid references zbusiness.zid
  end foreign key

  sql create view imtorreqview (zid,xtornum,xsrnum,xpreparer,xfwh,xlong,xtwh,xname,xrow,xitem,xdesc,xqtyreq,xqtyalc,xqtycrn)~
	as Select a.zid,a.xtornum,a.xtornum,a.xpreparer,a.xfwh,c.xlong,a.xtwh,d.xlong,b.xrow,b.xitem,e.xdesc,isnull(b.xqtyreq,0),isnull(b.xqtyalc,0),isnull(b.xqtycrn,0) from imtorheader a ~
	join imtordetail b on a.zid=b.zid AND a.xtornum=b.xtornum ~
	join branchview c on a.zid=c.zid and a.xfwh=c.xcode ~ 
	join branchview d on a.zid=d.zid and a.xtwh=d.xcode ~
	join caitem e on b.zid=e.zid and b.xitem=e.xitem 


end table
