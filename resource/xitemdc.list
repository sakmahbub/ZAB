     list xitemdc
        caption "Items containing: "+zvalue
        table caitem
        order xitem
		linksql select xdesc from caitem
        select "((xitem like '%"+zvalue+"%' or xdesc like '%"+zvalue+"%') and left(xitem,2)<>'CC') and zactive=1 and xgitem in('Chemical','Dyes')"
        searchselect "left(xitem,2)<>'CC' and zactive=1 and xgitem in('Chemical','Dyes')"
		rows 30
        objects xitem attrib(link "abc" embed onclick="return updateParent(this:'$')"), ~
                xdesc,xtheircode,xcatitem,xtex,xtkt,xdenier,xgitem //,xitemdept,xitemsubdept//,xshelf
     end list
