product ZABERP

table cabankviewnew
  caption "Bank View"
     columns zid, xbank,xname,xbranch,xbacc,xmadd,xacc,xswiftcode,xsub,xacctype
   primary key zid, xbank
  foreign key
        zid references zbusiness.zid
  end foreign key
 sql create view cabankviewnew (zid, xbank,xname,xbranch,xbacc,xmadd,xacc,xswiftcode,xsub,xacctype) ~
                  as select zid, xbank,xname,xbranch,xbacc,xmadd,xacc,xswiftcode,xsub,xacctype from cabank where xacc<>'' and xacc<>'0'
end table

