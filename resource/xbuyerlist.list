 list xbuyerlist
	 caption "BuyerList"
        table cacus
        order xcus
		linkobjects xcus
		linksql select xcus from cacus
        select "(xcus like '%"+zvalue+"%' or xorg like '%"+zvalue+"%') and xtpe='Buyer'"
        rows 20
		searchselect "xtype in('Buyer')"
        objects xcus attrib(link "abc" embed onclick="return updateParent(this:'')"), ~
			xorg,xmadd,xtype
               
	headers "Cus","Name","Address","Type"
     end list