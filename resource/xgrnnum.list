     list xgrnnum
        caption "Items containing: "+zvalue
        table pogrnheader
        order xgrnnum desc
        select "(xgrnnum like '%"+zvalue+"%' or xdate like '%"+zvalue+"%') and left(xgrnnum,4)='GRN-'"
		searchselect "left(xgrnnum,4)='GRN-'"
        rows 20
        objects xgrnnum attrib(link "abc" embed onclick="return updateParent(this:'$')"), ~
                xdate,xlcno
     end list


