     list xgiftitem
        caption "Items containing: "+zvalue
        table caitem
        order xitem
        select "((xitem like '%"+zvalue+"%' or xdesc like '%"+zvalue+"%') and left(xitem,2)<>'CC') and zactive=1"
        searchselect "left(xitem,2)<>'CC' and zactive=1"
        navigators alpha
        rows 10
        objects xitem attrib(link "abc" embed onclick="return updateParent(this:'$')"), ~
                xdesc



        //totals "","Totals","","","","",sum,sum

        //headers none
     end list


