     list inwsno
        caption "Transfer Order List"</b>"
        table imtorheader
        order xtornum
        select "(xtornum like '%"+zvalue+"%' and left(xtornum,4) in ('WRE-','WIS-')"
		searchselect "left(xtornum,4) in ('WRE-','WIS-')"
        rows 10
        objects ~
         xtornum attrib(link "abc" embed onclick="return updateParent(this:'')"),xdate,xordernum
		 
		headers "TO Number","Date" ,"Ref"
     end list


