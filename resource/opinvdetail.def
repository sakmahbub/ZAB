product ZAB ERP

table opinvdetail
  caption "Commercial Invoice Detail"
  columns ztime,zutime,zid,zauserid,zuuserid,~
          xinvnum,xinvrow,xdornum,xitem,xnote,xqtyinv,xrate,xlineamt,~
		  xpiref,xpidate,xgrossweight,xnoofrolls,xorderrow,xprcstype,xstyle,~
		  xcomposition,xfabrictype,xfabriccolor,xwidth,xgsm,~
		  xordernum,xnetweight,xctnno,xprodtype,xcolorcode,xbcolor,xordernumso,xqtyinvdup

  primary key zid,xinvnum,xinvrow

  foreign key
        zid references zbusiness.zid
        zid,xinvnum references opinvheader.zid,xinvnum
       zid,xitem references caitem.zid,xitem
  end foreign key

end table

