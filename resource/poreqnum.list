        order xporeqnum desc
        select "xporeqnum like '%"+zvalue+"%'"
        rows 20
        objects xporeqnum attrib(link "abc" embed onclick="return updateParent(this:'')"),~
                xdate,xtype,xname,xlong
				
				Headers "Requisition No","Date","Type","Preparer","Srore/ Project"
				
				

     end list


