     list xitemservice
        caption "Items containing: "+zvalue
        table caitem
        order xitem
		linksql select xdesc from caitem
        select "((xitem like '%"+zvalue+"%' or xdesc like '%"+zvalue+"%') and left(xitem,2)<>'CC') and zactive=1 and xgitem in ('Service Local','Service Foreign')"
        searchselect "left(xitem,2)<>'CC' and zactive=1	and xgitem in ('Service Local','Service Foreign')"
		rows 30
        objects xitem attrib(link "abc" embed onclick="return updateParent(this:'$')"), ~
                xdesc,xtheircode,xcatitem,xgitem//,xitemdept,xitemsubdept//,xshelf
     end list


