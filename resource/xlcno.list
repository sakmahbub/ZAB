     list xlcno
        table lcinfo
        order xlcno
        select "(xlcno like '%"+zvalue+"%') and xtype in ('Import')"
        searchselect "xtype in ('Import')"
		rows 20
        objects xlcno attrib(link "abc" embed onclick="return updateParent(this:'')"),~
                xtype,xlcissuedate,xcus//xpornum
		header "LC No","LC Type","LC Issue Date","Supplier ID" //"PO No",
     end list


