product ZAB

table imwtview
  caption "Inventory Transaction"
  columns zid,xitem,xwh,xqty,xval
  primary key zid,xitem,xwh
  foreign key
        zid references zbusiness.zid
        zid,xitem references caitem.zid,xitem
  end foreign key
  check
    //country  = 'US' or country = 'SG'
  end check
  //field xcus
    //extend
    //attrib trn Sales Order, 10
  //end field
  sql create view imwtview (zid,xitem,xwh,xqty,xval) ~
                  as select  zid, xitem,~
				xwh,sum(xqty*xsign),sum(xval*xsign) from imtrn ~
                  group by zid,xitem,xwh
end table
