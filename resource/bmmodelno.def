
product ZAB ERP

table bmmodelno
  caption "Ac Bal View"
  columns zid,xacc,xsub,xyear,xprime,xacctype,xwh,xhwh
  primary key zid,xacc,xsub,xyear
  foreign key
        zid references zbusiness.zid
  end foreign key

  sql create view bmmodelno (zid,xcode,xlong)~
                  as select zid,xcode,xlong ~
                  from xcodes where xtype='Asset Type'

end table
