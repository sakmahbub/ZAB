  columns ztime,zutime,zauserid,zuuserid,zid,xcrnnum,xrow, xitem,~
          xqtyord,xqtybonus,xrate,xunit,xlineamt,xlong,xordernum,xdocrow,~
	  xvatrate,xvatamt,xdiscdet,xdiscdetamt,xcost

