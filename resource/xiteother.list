   list xiteother
        caption "Items containing: "+zvalue
        table caitem
        order xitem
		linksql select xdesc from caitem
        select "((xitem like '%"+zvalue+"%' or xdesc like '%"+zvalue+"%') and left(xitem,2)<>'CC') and zactive=1 and xgitem not in ('Service Local','Service Foreign','Finished Goods')"
        searchselect "left(xitem,2)<>'CC' and zactive=1 and xgitem not in ('Service Local','Service Foreign','Finished Goods')"
		rows 30
        objects xitem attrib(link "abc" embed onclick="return updateParent(this:'$')"), ~
                xdesc,xtheircode,xcatitem,xgitem//,xitemdept,xitemsubdept//,xshelf
     end list

