     list xstaffgroup
        table pdmst
        order xstaff
        select "(xstaff like '%"+zvalue+"%' or xname like '%"+zvalue+"%') and xgroup<>'' and xgroup='"+xgroup+"'"
		searchselect "xgroup='"+xgroup+"' and xgroup<>''"
        rows 20
        objects xstaff attrib(link "abc" embed onclick="return updateParent(this:'')"),xposition, ~
                xname,xdesignation,xdeptname
     end list


