


product ZAB ERP

table poreqprview
  caption "Ac Sub View"
  columns zid,xporeqnum,xtornum,xwh,xlong,xregi,xdate
  primary key zid,xporeqnum,xtornum,xwh,xlong,xregi,xdate
  foreign key
        zid references zbusiness.zid
  end foreign key

  sql create view poreqprview (zid,xtornum,xporeqnum,xregi,xdate,xwh,xlong,xtype)~
                  as select a.zid,a.xtornum,b.xporeqnum,a.xregi,a.xdate,a.xfwh,c.xlong,b.xtype from  imtorheader a ~
					left join poreqheader b on a.zid=b.zid and a.xtornum=b.xtornum ~ 
					left join branchview c on a.zid=c.zid and a.xfwh=c.xcode
end table
