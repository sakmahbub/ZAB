     list xbinstock
        caption "Bin Ref: "+zvalue
        table imstockbinview
        order xbinref desc
        select "((xbinref like '%"+zvalue+"%') and xitem='"+xitem+"' and xwh='"+xfwh+"')"
		searchselect "xitem='"+xitem+"' and xwh='"+xfwh+"'"
		navigators alpha
		rows 20
        objects xbinref attrib(link "abc" embed onclick="return updateParent(this:'$')"), ~
                xitem,xdesc,xavail				
				
     end list


