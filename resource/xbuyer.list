list xbuyer
  table cacus
  order xcus
  linksql select xorg from cacus
        select "((xcus like '%"+zvalue+"%' or xorg like '%"+zvalue+"%') and xtype in('Buyer'))"
        rows 10
		searchselect "xtype in('Buyer')"
		objects xcus attrib(link "abc" embed onclick="return updateParent(this:'')"), ~
          xorg,xmadd,xtype

  header "Buyer ID","Buyer Name","Mailing Address","Type"
        
end list
