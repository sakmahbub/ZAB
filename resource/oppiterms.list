list oppiterms
        table zterms
		caption "Terms & Conditions"
        order xrow 
	  select "xnote like '%"+zvalue+"%' and (xtype='PI' and zactive='1')"
	  searchselect "xtype='PI' and  zactive='1'"
	  
        rows 20
        objects xrow attrib(link "abc" embed onclick="return updateParent(this:'')"),xtype,xnote
                
     end list
